$cust _countries = _Arr('IRAN','USA','ENGLAND','INDIA','NETHERLAND')
$int _index = 0
println 'Start To Print : '

to([_SizeOf(_countries*) - 1])
{
printd '{0} -> {1} ' << _index , _countries[_index]
_index += 1
newline >
}

print 'End .'

